{
  "manifest_version": 2,
  "name": "rust-doc-is-too-verbose",
  "description": "Reduce verbosity of rust doc",
  "version": "0.0.1",
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content_script.js"]
    }
  ],
  "page_action": {
    "browser_style": true,
    "default_icon": "icon.svg",
    "default_title": "Whereami?",
    "show_matches": ["*://*/*"]
  }
}
