{
  "manifest_version": 2,
  "name": "rust-doc-is-too-verbose",
  "description": "Reduce verbosity of rust doc",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.mozilla.org/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}
